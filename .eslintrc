{
  "globals": {
    "document": true,
    "localStorage": true,
  },
  "settings": {
    "import/resolver": "webpack"
  },
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [0],
    "react/prefer-stateless-function": [0],
    "react/prop-types": [0],
    "no-console": [0],
    "no-useless-constructor": [0],
    "arrow-parens": [0],
    "no-underscore-dangle": [0],
    "jsx-a11y/no-static-element-interactions": [0],
    "max-len": [0]
  }
}
